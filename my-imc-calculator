fun main() {
    calcularIMC(INTRODUCIR PESO, INTRODUCIR ALTURA) // Ejemplo (75.5, 1.75)
}

// Funci√≥n para calcular el indice de masa muscular
fun calcularIMC(peso: Double, altura: Double) {
    val alturaAlCuadrado: Double = altura * altura
    
    val imc = peso / alturaAlCuadrado
	  println("Tu IMC es de $imc")
}
